            GlobalVar.AxisPCI.MoveDIR(GlobalVar.AxisA.LinkIndex, true, (GlobalVar.Point_FeedRight) * GlobalVar.ServCMDRate, false);//运动到IC上料位置
            GlobalVar.AxisPCI.WaitAllMoveFinished();//等待运动完成
