        bool isFeedModbus = true;
        private void comboBox_Modbus_SelectedIndexChanged(object sender, EventArgs e)
        {
            if (comboBox_Modbus.SelectedIndex == 0)
            {
                isFeedModbus = true;
            }
            else isFeedModbus = false;
